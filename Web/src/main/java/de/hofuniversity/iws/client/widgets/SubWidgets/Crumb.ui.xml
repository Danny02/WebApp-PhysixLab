<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
        @font-face {
        font-family: geosans;
        src: url('../GeosansLight.ttf');
        }
    
        .crumbStyle {
        font-family: geosans;
        font-size: 14px;
        }
    </ui:style>
    <g:FocusPanel ui:field="action">
        <g:HTMLPanel>
            <p ui:field="crumb" class="crumbStyle"></p>
        </g:HTMLPanel>
    </g:FocusPanel>
</ui:UiBinder>