<!--<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">-->

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    
    <ui:with field="res" type="de.hofuniversity.iws.client.widgets.Images"></ui:with>
    <ui:with type="de.hofuniversity.iws.client.HistoryPageController.Tokens" field="tokens" />
  
    <ui:style>
    
    </ui:style>
    
    <g:ScrollPanel>
        <g:HTMLPanel >
            <g:HyperLink text="Go to User Info" targetHistoryToken="{tokens.userinfopage}"/>
            <g:Label ui:field='sessionLabel'></g:Label>
            <g:Button ui:field='logout'>Logout</g:Button>
        </g:HTMLPanel>
    </g:ScrollPanel>
</ui:UiBinder>
